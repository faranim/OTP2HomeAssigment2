<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="300.0" minWidth="200.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="org.example.otp2homeassigment2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button fx:id="button1" onAction="#onHelloButtonClick" prefHeight="26.0" prefWidth="144.0" text="Button 1" />
   <Button fx:id="button2" onAction="#onHelloButtonClick" prefHeight="26.0" prefWidth="141.0" text="Button 2" />
   <Button fx:id="button3" onAction="#onHelloButtonClick" prefHeight="26.0" prefWidth="144.0" text="Button 3" />

    <Label fx:id="welcomeText" />
</VBox>
